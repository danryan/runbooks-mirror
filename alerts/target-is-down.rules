## Target is down
ALERT target_is_down
  IF up{job!~"blackbox"} == 0
  FOR 1m
  LABELS {severity="critical"}
  ANNOTATIONS {
    title="{{ $labels.job }}/{{ $labels.instance }} failed to be scraped for the last minute",
    description="Check prometheus alerts at https://prometheus.gitlab.com/alerts and investigate targets at https://prometheus.gitlab.com/targets page to undestand which hosts or exporters could not be scraped",
  }
