## Sidekiq Queues
ALERT git_cat_file_high
  IF sum(process_count{name="git cat-file"}) >= 40
  FOR 5m
  LABELS {severity="warn", channel="infrastructure"}
  ANNOTATIONS {
    title="Number of 'git cat-file' process is high - {{$value}}",
    description="`git cat-file` processes are high for the 5 minutes, this may be under control, but I'm just letting you know that this is going on, check http://performance.gitlab.net/dashboard/db/fleet-overview?panelId=15&fullscreen."
  }
