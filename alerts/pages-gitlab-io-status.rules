## gitlab-examples.gitlab.io
ALERT gitlab_pages_is_down
  IF max(probe_success{instance=~"^pages.gitlab.io$", job="blackbox"}) != 1
  FOR 1m
  LABELS {severity="critical", pager="pagerduty"}
  ANNOTATIONS {
    title="GitLab pages is down for 1 minute",
    description="GitLab pages is down for 5 minutes! Consider !tweet 'GitLab pages are not responding, we are investigating the root cause.' if problem exists.",
    runbook="troubleshooting/gitlab-pages.md"
  }
